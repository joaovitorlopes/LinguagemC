#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(){
char nome[50], nome2[50];
scanf("%s",&nome);
scanf("%s",&nome2);
int total=0;
    while( nome[total] != '\0'){
        total++;
  }
int total2=0;
    while( nome2[total2] != '\0'){
        total2++;
    }
     if(total != total2){
     printf("As strings não são iguais\n");
    }else{
        for(int i = 0; i < total; i++){
          if(nome[i] != nome2[i]){
            printf("As strings não são iguais\n");
          }else{
            printf("As strings são iguais\n");
          }
       }
   }


    return 0;
}
