#include <stdio.h>

int main(){
int ve1[15], ve2[15], i, contador = 0;

for(i = 0; i < 15; i++){
  printf("Insira um número para casa %d:\n",i);
  scanf("%d",&ve1[i]);
}

for(i = 0; i < 15; i++){
  printf("Insira um número para casa %d:\n",i);
  scanf("%d",&ve2[i]);
}
for(i = 0; i < 15; i++) {
  if(ve1[i] == ve2[i]){
    contador = contador + 1;
  }
}
printf("Existem %d número(s) iguais dentro dos vetores\n", contador);




  return 0;
}
